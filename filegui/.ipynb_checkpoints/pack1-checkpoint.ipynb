   "source": [
    "from tkinter import *\n",
    "\n",
    "window = Tk()\n",
    "window.title('pack')\n",
    "window.geometry(\"300x100\")\n",
    "window.option_add('*Font', \"바탕체 15\")\n",
    "\n",
    "# pack() - 한 줄을 차지함\n",
    "Label(window, text='안녕하세요').pack()\n",
    "Button(window, text='확인')\n",
    "\n",
    "window.mainloop()\n"
   ]
